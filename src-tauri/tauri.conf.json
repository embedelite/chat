{
  "build": {
    "beforeDevCommand": "npm run start -- --port 1420",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist/embedelite-native",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "embedelite-native",
    "version": "0.2.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "startDragging": true
      }
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://api.github.com/repos/embedelite/chat/releases/latest"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDg4MkQzOUQ4RjM1M0I3OUYKUldTZnQxUHoyRGt0aUhRVjFQQlZrK1BmbGRxNFZwdjNXYnhnSTlOdi9LT2lpUkxYK3dxcnpFd04K"
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.embedelite.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "",
        "width": 1024,
        "height": 1024,
        "titleBarStyle": "Overlay"
      }
    ]
  }
}
